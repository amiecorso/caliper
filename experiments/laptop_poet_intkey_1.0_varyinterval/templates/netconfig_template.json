{
  "caliper":{
    "blockchain": "sawtooth",
    "command" : {
      "start": "ssh amie@{remoteip} \"python3 {exp_dir}start_tasks.py --n {n} --exp_dir {exp_dir} --run_num {run_num}\"", 
      "end" : "ssh amie@{remoteip} \"python3 {exp_dir}end_tasks.py --n {n} --exp_dir {exp_dir} --run_num {run_num} --leave_up {leave_up} --tps {tps} --interval {interval}\""
    }
  },
  "sawtooth": {
    "batchBuilders": {
      "{TPfamily}": {
        "1.0": "src/adapters/sawtooth/Application/{batchbuilder_file}"
      }
    },
    "network": {
      "restapi": {
        "urls": {restapi_urls} 
      },
      "validator": {
        "url": {validator_url} 
      }
    }
  }
}
