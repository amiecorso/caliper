{
  "caliper":{
    "blockchain": "sawtooth",
    "command" : {
      "start": "python3 {exp_dir}start_tasks.py --n {n} --exp_dir {exp_dir} --run_num {run_num}", 
      "end" : "python3 {exp_dir}end_tasks.py --n {n} --exp_dir {exp_dir} --run_num {run_num} --leave_up {leave_up} --tps {tps}"
    }
  },
  "sawtooth": {
    "batchBuilders": {
      "{TPfamily}": {
        "1.0": "src/adapters/sawtooth/Application/{batchbuilder_file}"
      }
    },
    "network": {
      "restapi": {
        "urls": {restapi_urls} 
      },
      "validator": {
        "url": "tcp://127.0.0.1:8800"
      }
    }
  }
}
